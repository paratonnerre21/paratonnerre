#include <iostream>

using namespace std;
int main() {


	int x;
	cout << "Please enter how many integers the array will contain: ";
	cin >> x;

	int* ptr = new int[x];

	for (int i = 0; i < x; i++) {
		cin >> ptr[i];

	}

	int sum = 0;
	for (int i = 0; i < x; i++) {
		sum = sum + ptr[i];
	}

	cout << "Sum: " << sum;







	cout << endl;
	return 0;
}
